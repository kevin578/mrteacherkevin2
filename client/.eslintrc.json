{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends":[ "eslint:recommended", "plugin:react/recommended", "prettier", "prettier/react"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "react/no-unescaped-entities":0,
        "react/prop-types": 0,
        "no-unused-vars": 0,
        "no-undef": 0
    }
}